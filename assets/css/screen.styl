body
	font-family 'Asap'
	background #f7f7f7

h2
	margin-top 0
header
	padding 24px 0
	background #fcfcfc

	.logo
		height 34px

	.name
		margin-right 24px

.navbar
	background #fcfcfc

	.navbar-nav .active > a
		margin-bottom -1px
		border-bottom 2px solid #3965aa
		background none
		color #777

.btn
	position relative
	-webkit-transition background 150ms
	transition background 150ms

	&.btn-default
		border 1px solid #d5d5d5
		background #fdfdfd
		background -webkit-gradient(linear, left top, left bottom, from(#fdfdfd), to(#f0f0f0))
		background -webkit-linear-gradient(#fdfdfd, #f0f0f0)
		background linear-gradient(#fdfdfd,#f0f0f0)
		color #525252
		-webkit-box-shadow inset #fff 0 1px
		box-shadow inset #fff 0 1px

		&:hover
			border 1px solid #d5d5d5
			background #fafafa
			background -webkit-gradient(linear, left top, left bottom, from(#fafafa), to(#e9e9e9))
			background -webkit-linear-gradient(#fafafa, #e9e9e9)
			background linear-gradient(#fafafa,#e9e9e9)

		&:active
		&.active
			background -webkit-gradient(linear, left top, left bottom, from(#bebebe), to(#e9e6e6))
			background -webkit-linear-gradient(#bebebe, #e9e6e6)
			background linear-gradient(#bebebe,#e9e6e6)
			-webkit-box-shadow none
			box-shadow none

	&.btn-primary
		border 1px solid #1f7c57
		text-shadow #1c8f3d 0 1px 0
		background #3dbe8b
		background -webkit-gradient(linear, left top, left bottom, from(#43c692), to(#39b885))
		background -webkit-linear-gradient(#43c692, #39b885)
		background linear-gradient(#43c692,#39b885)
		color #fff
		-webkit-box-shadow inset #4fd3aa 0 1px
		box-shadow inset #4fd3aa 0 1px

		&:hover
			border 1px solid #1f7c57
			background #2baa78
			background -webkit-gradient(linear, left top, left bottom, from(#33b481), to(#23a06e))
			background -webkit-linear-gradient(#33b481, #23a06e)
			background linear-gradient(#33b481,#23a06e)

		&:active
		&.active
			background #1a9263
			background -webkit-gradient(linear, left top, left bottom, from(#11774f), to(#1fa06d))
			background -webkit-linear-gradient(#11774f, #1fa06d)
			background linear-gradient(#11774f,#1fa06d)
			-webkit-box-shadow none
			box-shadow none
			color rgba(255,255,255,0.9)

	&.btn-danger
		border 1px solid #ac2125
		text-shadow 0 1px rgba(0,0,0,0.36)
		background #d95a4f
		background -webkit-gradient(linear, left top, left bottom, from(#eb745d), color-stop(6%, #de5042), color-stop(50%, #d95a4f), color-stop(90%, #d1483a), to(#cb3f33))
		background -webkit-linear-gradient(#eb745d 5%, #de5042 6%, #d95a4f 50%, #d1483a 90%, #cb3f33 95%)
		background linear-gradient(#eb745d 5%,#de5042 6%,#d95a4f 50%,#d1483a 90%,#cb3f33 95%)
		color #fff

		&:hover
			border-color #cd7576
			background #dd9e9f
			background -webkit-gradient(linear, left top, left bottom, from(#efc1c0), color-stop(6%, #e6a8a9), color-stop(90%, #dd9e9f), to(#d89293))
			background -webkit-linear-gradient(#efc1c0 5%, #e6a8a9 6%, #dd9e9f 90%, #d89293 95%)
			background linear-gradient(#efc1c0 5%,#e6a8a9 6%,#dd9e9f 90%,#d89293 95%)

		&:active
		&.active
			border-color #9a282a
			background #af1810
			background -webkit-gradient(linear, left top, left bottom, from(#830909), color-stop(10%, #af1810), to(#af1810))
			background -webkit-linear-gradient(#830909 0%, #af1810 10%, #af1810 100%)
			background linear-gradient(#830909 0%,#af1810 10%,#af1810 100%)

.panel.panel-spacious
	.panel-body
		padding-top 96px
		padding-bottom 96px

.CodeMirror
	display block
	width 100%
	font-size 14px
	line-height 1.42857143
	color #555
	background-color #fff
	background-image none
	border 1px solid #ccc
	border-radius 4px
	-webkit-box-shadow inset 0 1px 1px rgba(0,0,0,.075)
	box-shadow inset 0 1px 1px rgba(0,0,0,.075)
	-webkit-transition border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s
	-o-transition border-color ease-in-out .15s,box-shadow ease-in-out .15s
	transition border-color ease-in-out .15s,box-shadow ease-in-out .15s
